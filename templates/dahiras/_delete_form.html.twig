<form method="post" action="{{ path('app_dahiras_delete', {'id': dahira.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ dahira.id) }}">
    <button class="btn btn-danger app-btn-danger"><i class="fa fa-trash"></i></button>
</form>
