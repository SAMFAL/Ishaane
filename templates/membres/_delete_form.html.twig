<form method="post" action="{{ path('app_membres_delete', {'id': membre.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');" style="display: inline-block">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ membre.id) }}">
    <button class="btn btn-danger app-btn-danger"><i class="fa fa-trash text-white"></i></button>
</form>
